{
  "name": "ngx-form-error",
  "version": "1.0.0",
  "description": "Display error messages for angular forms",
  "author": "Michael Ressmann",
  "license": "MIT",
  "repository": "https://github.com/mmmichl/ngx-form-error",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-form-error --prod",
    "postbuild": "cp README.md dist/ngx-form-error",
    "test": "ng test ngx-form-error",
    "test:once": "ng test --watch=false ngx-form-error",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "cd projects/ngx-form-error; standard-version --infile ../../CHANGELOG.md",
    "commit-check": "npm run lint && npm run test:once && npm run build && ng build ngx-form-error-tester && npm run e2e"
  },
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "core-js": "^3.1.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.2",
    "@angular/cli": "~11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.1.1",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^6.0.1",
    "ts-node": "~8.2.0",
    "tslint": "~6.1.3",
    "typescript": "~4.1.3"
  }
}
